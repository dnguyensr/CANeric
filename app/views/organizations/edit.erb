<div >
  <div class = "errors">
    <% if @errors %>
      <% @errors.each do |msg| %>
      <%= msg %>
      <% end %>
    <% end %>
  </div>

  <h1>Edit Organization</h1>
  <form action="/organizations/<%= @organization.id %>" method="POST">
  <input type="hidden" name="_method" value="PUT" >
    <%= erb :'organizations/_organization_form' %>
    <input class="myButton" type="submit" value="Edit Organization">
  </form>
</div>
